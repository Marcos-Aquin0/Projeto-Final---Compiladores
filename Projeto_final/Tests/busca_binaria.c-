/* Encontra um valor em um vetor ordenado usando busca bin√°ria */
int vet[8];

int buscabinaria(int chave, int tam) {
    int baixo;
    int alto;
    int meio;
    baixo = 0;
    alto = tam - 1;

    while (baixo <= alto) {
        meio = (baixo + alto) / 2;
        if (chave < vet[meio]) {
            alto = meio - 1;
        } else {
            if (chave > vet[meio]) {
                baixo = meio + 1;
            } else {
                return meio;
            }
        }
    }
    return tam; 
}

void main(void) {
    vet[0] = 10; vet[1] = 20; vet[2] = 30; vet[3] = 40;
    vet[4] = 50; vet[5] = 60; vet[6] = 70; vet[7] = 80;

    output(buscabinaria(70, 8)); 
    saltoSO(); 
}